<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/doctor_nav_graph"
    app:startDestination="@id/doctorMainFragment">

    <fragment
        android:id="@+id/doctorMainFragment"
        android:name="com.example.smartgymapp.ui.doctor.dMain.DoctorMainFragment"
        android:label="fragment_doctor_main"
        tools:layout="@layout/fragment_doctor_main" />
    <fragment
        android:id="@+id/doctorProfileFragment"
        android:name="com.example.smartgymapp.ui.doctor.dProfile.DoctorProfileFragment"
        android:label="fragment_doctor_profile"
        tools:layout="@layout/fragment_doctor_profile" />
    <fragment
        android:id="@+id/doctorChatFragment"
        android:name="com.example.smartgymapp.ui.doctor.dChat.DoctorChatFragment"
        android:label="fragment_doctor_chat"
        tools:layout="@layout/fragment_doctor_chat" />
    <fragment
        android:id="@+id/doctorAccountFragment"
        android:name="com.example.smartgymapp.ui.doctor.dProfile.DoctorAccountFragment"
        android:label="DoctorAccountFragment"
        tools:layout="@layout/fragment_doctor_account">
        <action
            android:id="@+id/action_doctorAccountFragment_to_doctorProfileFragment"
            app:destination="@id/doctorProfileFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />

    </fragment>
</navigation>